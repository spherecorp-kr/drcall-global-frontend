# Hospital App - DEV Environment

# API Base URL (ALB → hospital-service)
# Host-based routing: hospital.{env}.drcall.global → hospital-service
VITE_API_BASE_URL=https://hospital.dev.drcall.global
VITE_HOSPITAL_API_URL=https://hospital.dev.drcall.global/api/v1

# Other services (accessed via hospital-service out ports)
VITE_PATIENT_API_URL=https://patient.dev.drcall.global/api/v1
VITE_APPOINTMENT_API_URL=https://hospital.dev.drcall.global/api/v1/appointments
VITE_VIDEOCALL_API_URL=https://hospital.dev.drcall.global/api/v1/videocalls
VITE_STORAGE_API_URL=https://hospital.dev.drcall.global/api/v1/storage
VITE_PAYMENT_API_URL=https://hospital.dev.drcall.global/api/v1/payments
VITE_MESSAGING_API_URL=https://hospital.dev.drcall.global/api/v1/messaging

# Environment
VITE_ENVIRONMENT=dev

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_USE_MOCK_DATA=false

# Sendbird Configuration
VITE_SENDBIRD_APP_ID=

# Sentry (optional)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=dev

# Analytics (optional)
VITE_GA_TRACKING_ID=
